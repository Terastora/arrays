	cin >> i[RpH];
	
	cout << "Hour per Days: ";
	cin >> i[HpD];
	
	cout << "Number of Days: ";
	cin >> i[NumDays];
	}

	for (int i=0 ; i<5; i++)
	cout <<"\nName :"<< name[i] << "\tRate per Hour :"<<RpH[i]<< "\tHours per Day :"<< HpD[i] << "\tNumer of Days :"<<NumDays[i];
	

	return 0;
}

